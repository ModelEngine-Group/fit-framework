{
  "permissions": {
    "defaultMode": "acceptEdits",
    "allow": [
      "Bash(tree:*)",
      "WebSearch",
      "WebFetch",
      "Bash(gh:*)",
      "Bash(git:*)",
      "Bash(mvn clean install:*)",
      "Bash(npm:*)",
      "Bash(find:*)",
      "Bash(cat:*)",
      "Bash(curl:*)",
      "Bash(pkill:*)",
      "Bash(chmod:*)",
      "Bash(xargs:*)",
      "Bash(ls:*)",
      "Bash(pwd)",
      "Bash(echo:*)",
      "Bash(mkdir:*)",
      "Bash(cd:*)",
      "Bash(sleep:*)",
      "Bash(tail:*)",
      "Bash(head:*)",
      "Bash(build/bin/fit start:*)",
      "Bash(rm:*)"
    ],
    "deny": [
      "Bash(sudo rm:*)",
      "Read(.env)",
      "Read(.env.*)",
      "Read(./secrets/**)",
      "Read(./**/*.key)",
      "Read(./**/*.pem)",
      "Read(./**/credentials.*)"
    ],
    "ask": [
      "Bash(git push origin main)",
      "Bash(git push origin master)",
      "Bash(git push --force:*)",
      "Bash(git rebase:*)",
      "Bash(npm publish:*)",
      "Bash(mvn deploy:*)"
    ]
  }
}
